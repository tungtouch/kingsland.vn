  <!-- Content
  ============================================= -->
  <section id="content">

    <div class="content-wrap nopadding" style="background: #f5f5f5;">
      <div id="section-rent" class="center page-section  nobottompadding">
        <div class="container clearfix">
          <% datas.forEach(function(data) { %>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 item">
              <a href="/l/<%= data.slug %>" class="thumbnail">
                              <span class="screen">
                                  <img src="/house/1_1445328144.jpg" alt="...">
                                  <span class="status label label-success">ĐANG TRỐNG</span>
                              </span>
                <div class="description">
                  <span class="label label-primary"><%= data.for_lease.vanphong %></span>
                  <span class="price label label-danger price-format-vnd"><%= data.amount %></span>
                  <span class="label label-default"><%= data.area %>m <sup>2</sup></span>
                </div>

                <div class="caption">
                  <h6><%= data.house_number %> - <%= data.address %> - <%= data.dist %></h6>
                </div>
              </a>
            </div>
          <% }); %>

        </div>
      </div>




